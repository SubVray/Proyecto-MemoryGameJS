{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "api/cards.js", "use": "@vercel/node" },
    { "src": "api/scores.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/cards/(.*)", "dest": "api/cards.js" },
    { "src": "/api/scores", "dest": "api/scores.js" }
  ]
}
